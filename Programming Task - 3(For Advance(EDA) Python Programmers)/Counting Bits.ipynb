    "numbers = [2, 5]\n",
    "for n in numbers:\n",
    "    result = [0] * (n + 1)\n",
    "    for i in range(1, n + 1):\n",
    "        if i % 2 == 0:\n",
    "            result[i] = result[i // 2]\n",
    "        else:\n",
    "            result[i] = result[i // 2] + 1\n",
    "    print(result)"
   ]
